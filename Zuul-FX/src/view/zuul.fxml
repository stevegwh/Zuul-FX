<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#startDefaultGame" text="Start Default Game" />
                  <MenuItem fx:id="menuItemStartCustomGame" disable="true" mnemonicParsing="false" onAction="#startGame" text="Start Custom Game" />
            <MenuItem mnemonicParsing="false" onAction="#openFileChooser" text="Load Custom Game" />
            <MenuItem mnemonicParsing="false" onAction="#quitProgram" text="Quit" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
      <HBox prefHeight="567.0" prefWidth="900.0">
         <children>
            <TextArea fx:id="gameText" disable="true" prefHeight="100.0" prefWidth="726.0" style="-fx-opacity: 1;">
               <font>
                  <Font name="DejaVu Sans Mono" size="13.0" />
               </font></TextArea>
            <VBox prefHeight="489.0" prefWidth="254.0">
               <children>
                  <TextField id="sentence-builder" alignment="CENTER" disable="true" layoutX="10.0" layoutY="475.0" prefHeight="0.0" prefWidth="900.0" style="-fx-opacity: 1;" text="Inventory" />
                  <ListView id="inventory" fx:id="inventory" prefHeight="200.0" prefWidth="200.0" />
                  <TextField id="sentence-builder" alignment="CENTER" disable="true" prefHeight="0.0" prefWidth="900.0" style="-fx-opacity: 1;" text="Items In Room" />
                  <ListView id="itemsInRoom" fx:id="itemsInRoom" prefHeight="200.0" prefWidth="200.0" />
                  <TextField id="sentence-builder" alignment="CENTER" disable="true" layoutX="10.0" layoutY="474.0" prefHeight="0.0" prefWidth="900.0" style="-fx-opacity: 1;" text="Actors In Room" />
                  <ListView id="actorsInRoom" fx:id="actorsInRoom" prefHeight="200.0" prefWidth="200.0" />
               </children>
            </VBox>
         </children>
      </HBox>
      <TextField id="sentence-builder" alignment="CENTER" layoutX="10.0" layoutY="39.0" prefHeight="0.0" prefWidth="900.0" text="Hello" />
      <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button id="north" fx:id="buttonGoNorth" disable="true" mnemonicParsing="false" onMouseClicked="#goClicked" text="North" textAlignment="CENTER">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button id="east" fx:id="buttonGoEast" disable="true" mnemonicParsing="false" onMouseClicked="#goClicked" text="East" textAlignment="CENTER">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button id="south" fx:id="buttonGoSouth" disable="true" mnemonicParsing="false" onMouseClicked="#goClicked" text="South" textAlignment="CENTER">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button id="west" fx:id="buttonGoWest" disable="true" mnemonicParsing="false" onMouseClicked="#goClicked" text="West" textAlignment="CENTER">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button fx:id="buttonLook" disable="true" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onMouseClicked="#lookClicked" text="Look" textAlignment="CENTER">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button fx:id="buttonGive" disable="true" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" text="Give" textAlignment="CENTER">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
      </HBox>
    <HBox id="HBox" alignment="CENTER_LEFT" spacing="5.0" VBox.vgrow="NEVER">
      <children>
        <Label maxHeight="1.7976931348623157E308" maxWidth="-1.0" text="Left status" HBox.hgrow="ALWAYS">
          <font>
            <Font size="11.0" fx:id="x3" />
          </font>
          <textFill>
            <Color blue="0.625" green="0.625" red="0.625" fx:id="x4" />
          </textFill>
        </Label>
        <Pane prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
        <Label font="$x3" maxWidth="-1.0" text="Right status" textFill="$x4" HBox.hgrow="NEVER" />
      </children>
      <padding>
        <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
      </padding>
    </HBox>
  </children>
</VBox>
