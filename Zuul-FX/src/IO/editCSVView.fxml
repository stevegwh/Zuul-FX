<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.EditCSVController">
  <children>
      <MenuBar>
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem fx:id="undoMenuItem" mnemonicParsing="false" onAction="#undoAction" text="Undo" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <HBox prefHeight="442.0" prefWidth="640.0">
         <children>
            <VBox prefHeight="375.0" prefWidth="478.0">
               <children>
                  <TextArea fx:id="csvText" prefHeight="402.0" prefWidth="461.0" />
               </children></VBox>
            <VBox prefHeight="375.0" prefWidth="197.0">
               <children>
                  <Button alignment="TOP_LEFT" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#addItemToAllRooms" prefHeight="25.0" text="Add Item To All Rooms" />
                  <Button alignment="TOP_LEFT" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#removeAllWithoutExit" prefHeight="25.0" prefWidth="141.0" text="Remove All Rooms w/o Exits" />
                  <Button alignment="TOP_LEFT" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#removeAllWithoutItem" prefHeight="25.0" prefWidth="152.0" text="Remove All Rooms w/o Items" />
               </children>
            </VBox>
         </children>
      </HBox>
  </children>
</VBox>
